{
  "name": "@meeco/sdk",
  "description": "A wrapper library around Meeco's various API SDK's and encryption libraries",
  "version": "2.0.1",
  "author": "Meeco",
  "bugs": "https://github.com/Meeco/cli/issues",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "@meeco/cryppo": "^2.0.0",
    "@meeco/keystore-api-sdk": "^5.3.0-stage",
    "@meeco/vault-api-sdk": "^22.0.0-stage",
    "@types/node-forge": "~0.9.0",
    "@types/parameterize": "^1.0.1",
    "base58-string": "0.0.3",
    "buffer": "5.6.0",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "jsrp": "~0.2.4",
    "node-fetch": "^2.6.0",
    "node-forge": "0.10.0",
    "parameterize": "^1.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/test": "^1.2.6",
    "@types/chai": "~4.2.7",
    "@types/debug": "^4.1.5",
    "@types/jsrp": "~0.2.1",
    "@types/mocha": "~5.2.7",
    "@types/node": "^10.14.18",
    "@types/sinon": "~7.5.1",
    "@types/yaml": "~1.2.0",
    "bili": "^4.10.0",
    "mocha": "^8.1.3",
    "nock": "~11.7.2",
    "nyc": "~15.0.0",
    "prettier": "~1.19.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "sinon": "~8.1.1",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "0.17.0-3",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/Meeco/cli",
  "keywords": [
    "meeco",
    "sdk",
    "meeco sdk"
  ],
  "license": "MIT",
  "repository": "Meeco/cli",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && bili src/index.ts --format cjs --format es --module-name meeco-sdk",
    "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" *.json README.md",
    "lint": "tslint -p . -t stylish -c ../../tslint.json && tslint -p test/tsconfig.json -t stylish -c ../../tslint.json",
    "posttest": "npm run lint && npm run format:check",
    "prepack": "npm run build",
    "build:typedoc": "typedoc --inputFiles src/index.ts --mode library --out ../../docs/js-sdk --tsconfig tsconfig.json --ignoreCompilerErrors --disableOutputCheck --name 'Meeco SDK Documentation' --toc AuthData,ConnectionService,DelegationService,EncryptionKey,Environment,InvitationService,ItemService,Secrets,Service,TemplatesService,UserService --excludePrivate",
    "docs": "npm run build:typedoc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
