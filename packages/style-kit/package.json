{
  "name": "@meeco/style-kit",
  "version": "1.0.0",
  "description": "Meeco branded components and style mixins",
  "main": "dist/index.css",
  "scripts": {
    "build": "webpack --mode production",
    "demo": "webpack-dev-server demo",
    "cypress": "cypress open",
    "docs": "build-storybook -c .storybook -o ../../docs/style-kit",
    "start": "npm run storybook",
    "storybook": "start-storybook -s . -p 8085 --ci",
    "test": "start-server-and-test 8085 'cypress run'",
    "update-snapshots": "start-server-and-test 8085 'cypress run --env updateSnapshots=true'",
    "posttest": "npm run format:check",
    "format:check": "prettier -c \"{stories,cypress,src}/**/*.{ts,html,scss,js}\"",
    "format:write": "prettier --write \"{stories,cypress,src}/**/*.{ts,html,scss,js}\" *.{json,scss} README.md"
  },
  "author": "Meeco",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@cypress/skip-test": "^2.6.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^6.2.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "color": "^3.1.3",
    "cypress": "^4.12.1",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-storybook": "^0.5.1",
    "js-base64": "3.6.0",
    "mini-css-extract-plugin": "^1.6.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "start-server-and-test": "^1.12.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
